<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <description>Practise</description>
        <display-name>Practise2</display-name>
        <servlet-name>practise2</servlet-name>
        <servlet-class>com.company.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>practise2</servlet-name>
        <url-pattern>/AdvancedJava</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>Practise1</description>
        <display-name>Practise3</display-name>
        <servlet-name>practise3</servlet-name>
        <servlet-class>com.company.SendServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>practise3</servlet-name>
        <url-pattern>/Send</url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>IDE</param-name>
        <param-value>Eclipse</param-value>
    </context-param>

</web-app>